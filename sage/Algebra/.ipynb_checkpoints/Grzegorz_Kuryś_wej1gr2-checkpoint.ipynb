   "source": [
    "A= matrix(3,3,[1,-1,-3,3,4,9,4,8,1])\n",
    "B= matrix(3,4,[0,-2,2,-1,2,1,-3,2,0,8,8,0])\n",
    "C= matrix(4,2,[3,3,3,-2,2,-2,-3,6])\n",
    "D= matrix(3,2,[-2,-2,3,5,6,4])\n",
    "show(A,B,C,D)"
   ]
